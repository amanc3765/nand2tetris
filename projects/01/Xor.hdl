/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a,b=b,out=t1);
    Nand(a=a,b=t1,out=t2);
    Nand(a=t1,b=b,out=t3);
    Nand(a=t2,b=t3,out=out);
}